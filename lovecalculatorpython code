# ðŸš¨ Don't change the code below ðŸ‘‡
print("Welcome to the Love Calculator!")
name1 = input("What is your name? \n")
name2 = input("What is their name? \n")
# ðŸš¨ Don't change the code above ðŸ‘†

#Write your code below this line ðŸ‘‡
name1lower = name1.lower()
name2lower = name2.lower()
bothname = name1lower + name2lower


truecounts = str(bothname.count('t')+bothname.count('r')+bothname.count('u')+bothname.count('e'))

lovewrdcounts = str(bothname.count('l')+bothname.count('o')+bothname.count('v')+bothname.count('e'))

lovecount =int(truecounts + lovewrdcounts)


if lovecount<10 or lovecount>90:
    print(f"Your score is {lovecount},you go together like coke and mentos.")
elif lovecount >=40 and lovecount<=  50:
    print(f"Your score is {lovecount},you are alright together.")
else:
    print(f"Your score is {lovecount}")


